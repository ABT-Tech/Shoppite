                            <div class="d-flex justify-content-between categoriescount">
                                <div class="form-check">
                                    <input class="form-check-input border-black border-1 rounded category" type="checkbox" name="CategoryName[]"
                                           value="@item.CategoryId" id="Ethnic Wear">
                                    <label class="form-check-label subnamefilter" for="Ethnic Wear">
                                        @item.CategoryName
                                    </label>
                                </div>
                                <span id="cat_@item.CategoryId" class="totalavailability">
                                    @foreach (var count in Model.ProductCount)
                                    {
                                        if (item.CategoryId == count.Category_Id)
                                        {
                                            @count.ProductCount
                                        }                                     
                                    }
                                </span>
                            </div>
                        }
