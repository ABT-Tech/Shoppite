@model Shoppite.Application.Models.MainModel
<style>
    .responsive {
        height: auto;
    }
</style>
@if (Model.MiddelBanner != null)
{
    @foreach (var image in Model.MiddelBanner.TakeLast(2))
    {
        if (Model.MiddelBanner.Count == 2)
        {
            <div class="col-md-6">
                <div class="shrink image">
                    <a  asp-action="AllProducts" asp-route-CategoryId="@image.CategoryId" name="middlebannerlink"><img src="@image.Image" loading="lazy" class="rounded-2 mt-2 responsive img-fluid lazy" alt="middelimage" width="645" height="215" /></a>
                </div>
            </div>
        }
    }
}

